    "binder": {
        "className": "org.joget.plugin.enterprise.JdbcDataListBinder",
        "properties": {
            "jdbcDatasource": "default",
            "optimisePaging": "",
            "primaryKey": "procId",
            "sql": "SELECT p.id AS idProc, sp.Id AS processId, sact.id, dp.c_n_wf_banco AS NwfBanco, dp.c_n_wf_pl AS NwfPL, dp.c_pid AS PID, dp.c_nome_banco AS NomeBanco, dp.c_tipo_processo as TipoProcesso, dp.c_origem_processo as OrigemProcesso,\n       dp.c_urgencia_registo AS Urgencia,  dp.c_n_wf_pl_relacionado AS ProcessosRelacionados, dp.c_categoria_processo as CategoriaProcesso, dp.c_fase_processo AS FaseProcesso, dp.c_tipo_entidade as Entidade,\n\t   d.c_necessarias_correcoes AS CorrecoesRegistos,  d.c_observacoes_correcao AS ObservacoesRegistos,\n       g.c_estado_processamento_mod11 AS EstadoProcessamento,\n       dp.dateCreated AS DataCriacao, dp.dateModified AS DataModificacao, sass.ResourceId AS assignee, ip.c_nome AS cliente,\n       ato.c_status as EstadoAgendamento, CONCAT(ato.c_dataOutorga, ' ', ato.c_horaOutorga) AS DataAgendamento, ato.c_dataOutorga, ato.c_horaOutorga, ato.c_cedula_entidade as entidadeAutenticadora, cl.c_nome_completo AS NomeEntidadeAutenticadora,\n       sp.Name AS processName, sact.Id AS instanceId, sact.Name AS activityName,\n       ROW_NUMBER() OVER (ORDER BY ato.c_dataOutorga ASC, ato.c_horaOutorga ASC, dp.c_urgencia_registo DESC) AS No\nFROM shkprocesses sp\nLEFT JOIN shkprocessstates sps ON sps.oid = sp.State\nLEFT JOIN SHKActivities sact ON sact.ProcessId = sp.Id\nLEFT JOIN SHKActivityStates ssta ON ssta.oid = sact.State\nLEFT JOIN SHKAssignmentsTable sass on sact.Id = sass.ActivityId\nLEFT JOIN app_fd_processos p on sp.ActivityRequesterProcessId = p.id\nLEFT JOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nLEFT JOIN app_fd_intervenientes_proc ip ON ip.c_id_processo = p.id AND ip.c_cliente_principal = 'CHECK'\nLEFT JOIN app_fd_agendamento ag ON  ag.id = p.c_agendamento\nLEFT JOIN app_fd_atos ato ON ato.c_id_agendamento = ag.id \nLEFT JOIN app_fd_geral g ON g.c_id_processo = p.id\nLEFT JOIN app_fd_docs_processo d ON  d.id = p.c_docs\nLEFT JOIN app_fd_info_colaboradores cl ON ato.c_cedula_entidade = cl.c_cedula_profissional AND ato.c_cedula_entidade IS NOT NULL AND ato.c_cedula_entidade <> ''\nWHERE sps.KeyValue LIKE 'open%'\nAND (ssta.oid = '1000001' OR ssta.oid = '1000003' OR ssta.oid = '1000005')\nAND sp.id not like '%Credito%'\nAND (ato.dateCreated = (SELECT MAX(dateCreated) FROM app_fd_atos a2 WHERE a2.c_id_processo = p.id) || ato.dateCreated IS null)\nAND (Select count(ID) from shkprocesses \n    where ActivityRequesterProcessId = sp.ActivityRequesterProcessId\n    AND (ID like '%Registos%'\n\t     OR ID like '%AcompanhamentoOutorgas%')\n) > 0"
        }
    },
