                            "className": "org.joget.apps.form.lib.BeanShellValidator",
                            "properties": {
                                "decoration": "*",
                                "script": "import org.joget.apps.app.service.AppUtil;\r\nimport org.joget.apps.form.model.Element;\r\nimport org.joget.apps.form.model.FormData;\r\nimport org.joget.apps.form.model.Form;\r\nimport org.joget.commons.util.LogUtil;\r\nimport org.joget.apps.form.service.FormUtil;\r\n\r\npublic boolean validacaoReprocessar(Element element, FormData formData){\r\n\r\n    //Configure field to validate - UPDATE HERE\r\n    String fieldId = \"reprocessar_mod11\";\r\n    String campo = FormUtil.getElementParameterName(element);\r\n    \r\n    String subfaseProcesso = \"#assignment.activityName#\";\r\n    Boolean result = false;\r\n    \r\n    //Get element value\r\n    Form form = FormUtil.findRootForm(element);\r\n    Element field = FormUtil.findElement(fieldId, form, formData);\r\n    String fieldValue = FormUtil.getElementPropertyValue(field, formData);\r\n    \r\n    if(subfaseProcesso.equals(\"Processamento Manual Modelo\")){\r\n        if (fieldValue == null || fieldValue.equals(\"\") ||fieldValue.equals(\"PV\")){\r\n            formData.addFormError(campo, \"Campo obrigatório\");\r\n            result = false;\r\n        } else{\r\n            result = true;  \r\n        }\r\n    } else{\r\n       result = true; \r\n    }\r\n\r\n    \r\n    return result;\r\n}\r\n\r\n//call load method with injected variable\r\nreturn validacaoReprocessar(element, formData);"
