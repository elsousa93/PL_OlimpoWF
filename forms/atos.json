                    {
                        "className": "org.joget.apps.form.lib.CustomHTML",
                        "properties": {
                            "autoPopulate": "",
                            "id": "field23",
                            "label": "",
                            "value": "<script>\n    $(document).ready(function(){\n        removerStatusOptions();\n        $(\"div#section2\").append('<div id=\"\" class=\"form-column form-column-horizontal\" style=\"width: \"><div class=\"form-cell\"><i class=\"form-button btn button  waves-input-wrapper waves-effect btn waves-button waves-float\" style=\"color:rgb(255, 255, 255);background:rgba(0, 0, 0, 0)\"><input id=\"sendEmail\" class=\"waves-button-input\" type=\"button\" value = \"ENVIAR EMAIL\"><\/i><\/div><\/div')\n        visibilidadesStatus();\n        \n        $(\"select[id*=status]\").change(function(){\n            visibilidadesStatus();\n        });\n        \n        $(\"select[id*=motivo_ato]\").change(function(){\n            visibilidadesStatus();\n        });\n        \n        $(\"input[id*=sendEmail]\").click(function(e){\n            e.preventDefault();\n            var status =  $(\"select[id*=status]\").val();\n            if(status === 'DES'){\n                envioEmailDesmarcacao();\n            }\n        });\n        \n       $(\"form#atos :input\").change(function(){\n            $(\"input#sendEmail\").prop(\"disabled\", false);\n            \n        });\n        \n        \n    });\n    \n    $(window).load(function(){\n        visibilidadesStatus();\n        //Ao submeter o formulário guardar a data de comunicação\n        $(\"input[id*=data_envio_agenda]\").prop(\"disabled\", true);\n        $(\"form#atos\").submit(function(){\n            $(\"input[id*=data_envio_agenda]\").prop(\"disabled\", false);\n        });\n    });\n    \n    \n    \n    \n    \n    function visibilidadesStatus(){\n        var status = $(\"select[id*=status]\").val();\n        var motivoAto = $(\"select[id*=motivo_ato]\").val();\n        \n        //Visibilidade do Motivo\n       /* if(status === '' || status === 'AGN' || status === 'AGD' || status === 'ACOM' || status === 'RJD'){\n            $(\"select[id*=motivo_ato]\").parent().hide();\n        }else{\n            $(\"select[id*=motivo_ato]\").parent().show();\n        }*/\n        \n        //Visibilidade do Botão de enviar email\n        if(status !== 'DES' || motivoAto == ''){\n            $(\"input[id*=sendEmail]\").parent().hide();\n        }else{\n           \n            $(\"input[id*=sendEmail]\").parent().show();\n        }\n    }\n    \n    function envioEmailDesmarcacao(){\n        var nWfAgenda = $(\"input[id*=n_wf_agendamento]\").val();\n        var motivoDesmarcacao = $(\"select[id*=motivo_ato] option:selected\").text();\n        var obsAutomaticas = $(\"textarea[id*=observacoes_agendamento]\").val();\n        \n        var msg = \"Pedido de desmarcação\\n\" + obsAutomaticas + \"\\nMotivo de desmarcação: \" + motivoDesmarcacao;\n        var subject = \"Pedido de desmarcação - \" + nWfAgenda;\n        var to = \"#envVariable.emailEnvioAgenda#\";\n        $.ajax({\n                type:\"POST\",\n                url: \"/jw/api/process/sendEmail/startProcess\",\n                headers:{\n                    \"api_id\": \"#envVariable.apiIdAtualizarCampos#\",\n                    \"api_key\": \"#envVariable.apiKeyAtualizarCampos#\",\n                    \"Accept\":\"application/json\",\n                    \"Content-Type\": \"application/json\"\n                },data: JSON.stringify({\n                    \"email_msg\": msg,\n                    \"subject\": subject,\n                    \"to\": to\n                }),\n                success: function(response){\n                    //Atualização da data de envio de email\n                    $(\"input[id*=data_envio_agenda]\").prop(\"disabled\", false);\n                    $(\"input[id*=data_envio_agenda]\").val(\"#date.dd-MM-yyyy HH:mm#\");\n                    $(\"input[id*=data_envio_agenda]\").prop(\"disabled\", true);\n                    $(\"input#sendEmail\").prop(\"disabled\", true);\n                }\n            });\n        \n    }\n    \n    function removerStatusOptions(){\n        var status = $(\"select[id*=status]\").val();\n        if(status === 'AGN'){\n            $(\"select[id*=status] option[value=RJD],[value=AGD],[value=OUT],[value=NOUT],[value=DESC],[value=DESA],[value=OUTI]\").remove();\n \n        }else if(status === 'AGD'){\n            $(\"select[id*=status] option[value=RJD],[value=OUT],[value=NOUT],[value=AGN],[value=DESC],[value=DESA]\").remove();\n            \n        }else if(status === 'RJD'){\n             $(\"select[id*=status] option[value=DESC],[value=AGN],[value=AGD],[value=DES],[value=OUT],[value=NOUT],[value=DESA],[value=OUTI]\").remove();\n            \n        }else if(status === 'DES'){\n            \n            /*var statusBd = $(\"form#atosAgendamento\").children(\"input[type=hidden][name=_jsonFormData]\").val().status;\n            console.log(\"STATUS BD \" + statusBd);\n            if(statusBd !== 'AGD'){\n                 $(\"select[id*=status] option[value=ADG]\").remove();\n            }else if(statusBd !== 'AGN'){\n                $(\"select[id*=status] option[value=AGN]\").remove();\n            }*/\n            \n            $(\"select[id*=status] option[value=RJD],[value=AGN],[value=AGD],[value=DESC],[value=OUT],[value=NOUT],[value=DESA],[value=OUTI]\").remove();\n        }else if(status === 'OUT'){\n            //$(\"select[id*=status] option[value=RJD],[value=AGD],[value=AGN],[value=DESC]\").remove();\n        }else if(status === 'NOUT'){\n            //$(\"select[id*=status] option[value=RJD],[value=AGD],[value=AGN],[value=DESC]\").remove();\n        }else if(status === 'DESC'){\n            $(\"select[id*=status] option[value=RJD],[value=AGN],[value=AGD],[value=DES],[value=OUT],[value=NOUT],[value=DESA],[value=OUTI]\").remove();\n        }\n        \n        \n        /*\n        else if(status !== ''){\n            //Caso já haja uma opção preenchida remover opção em branco\n            $('select[id*=status] option').filter(function(){\n                if($(this).val() === ''){\n                    $(this).remove();\n                }\n            });\n        }\n        */\n    \n    }\n<\/script>"
